    {
      "cell_type": "code",
      "metadata": {
        "id": "fmWK__hpWX31"
      },
      "source": [
        "# 각 단어에 고유 index 부여\n",
        "word_to_index = {word[0] : index + 2 for index, word in enumerate(vocab)}\n",
        "word_to_index['pad'] = 1\n",
        "word_to_index['unk'] = 0"
      ],
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
