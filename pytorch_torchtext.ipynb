      },
      "source": [
        "# tokenize 함수 정의\n",
        "def tokenize(en_text):\n",
        "    return [tok.text for tok in spacy_en.tokenizer(en_text)]\n",
        "\n",
        "en_text = \"A Dog Run back corner near spare bedrooms\"\n",
        "\n",
        "tokenize(en_text)"
      ],
