    {
      "cell_type": "code",
      "metadata": {
        "id": "Bu2Pr5M_XFQm"
      },
      "source": [
        "# vocab index로 문장 encoding\n",
        "encoded = []\n",
        "for line in tokenized:\n",
        "    temp = []\n",
        "    for w in line: \n",
        "      try:\n",
        "        temp.append(word_to_index[w]) # 글자를 해당되는 정수로 변환\n",
        "      except KeyError: # 단어 집합에 없는 단어일 경우 unk로 대체된다.\n",
        "        temp.append(word_to_index['unk']) # unk의 인덱스로 변환\n",
        "\n",
        "    encoded.append(temp)"
      ],
      "execution_count": null,
      "outputs": []
    },
